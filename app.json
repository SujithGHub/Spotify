{
  "expo": {
    "name": "Spotify",
    "slug": "spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Spotify_Primary_Logo_RGB_Green.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/Spotify_Primary_Logo_RGB_Green.png",
      "resizeMode": "contain",
      "backgroundColor": "#00000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.spotify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Spotify_Primary_Logo_RGB_Green.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.spotify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/Spotify_Primary_Logo_RGB_Black.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/Spotify_Primary_Logo_RGB_Green.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}